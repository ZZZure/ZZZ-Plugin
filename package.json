{
  "name": "zzz-plugin",
  "type": "module",
  "version": "2.0.0",
  "description": "绝区零云崽插件",
  "main": "index.js",
  "keywords": [],
  "author": "ZZZure",
  "scripts": {
    "build": "node build.js",
    "build:src": "node build.js --only-src",
    "build:css": "sass resources:resources --update --no-error-css",
    "watch": "nodemon -w src -e js,ts --exec pnpm run build",
    "watch:src": "nodemon -w src -e js,ts --exec pnpm run build:src",
    "watch:css": "sass resources:resources --watch --no-error-css",
    "check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\" \"src/**/*.js\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"src/**/*.js\" --fix"
  },
  "imports": {
    "#interface": "./src/@types/interface.d.ts"
  },
  "dependencies": {
    "source-map-support": "^0.5.21"
  },
  "peerDependencies": {
    "chokidar": "^4.0.3",
    "icqq": "*",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "node-fetch": "^3.3.2",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/lodash": "^4.17.21",
    "@types/md5": "^2.3.6",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "chokidar": "^4.0.3",
    "eslint": "^9.39.2",
    "globals": "^17.1.0",
    "icqq": "0.6.10",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.11",
    "sass": "^1.97.3",
    "typescript": "^5.9.3",
    "yaml": "^2.8.0"
  }
}